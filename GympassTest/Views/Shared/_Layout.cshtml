    <link href="https://fonts.googleapis.com/css?family=Exo+2:400,500,600,700,700i,800,800i,900" rel="stylesheet">s
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
